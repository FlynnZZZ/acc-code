# jQuery .js-------------------------------------------------------------------- 
'.source.js':
  '-jq-$($1)':
    'prefix': 'jq'
    'body': '$($1)'
  '-jq-$-jQuery($1)':
    'prefix': 'jQuery'
    'body': 'jQuery($1)'
  '-jq-$-$.Deferred();':
    'prefix': 'deferred'
    'body': '$.Deferred();'
  '-jq-$-$.getScript(url,cfoo);':
    'prefix': 'getScript'
    'body': """$.getScript("../js.js",function(){
        $1
      });""" 
