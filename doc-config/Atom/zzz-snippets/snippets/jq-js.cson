  '-jq-05-ajax':
    'prefix': 'ajaxJQ'
    'body': """$.ajax({ // 某接口
        type : 'GET',
        url  : 'url',
        data : {
          key : val,
        }, 
        dataType : 'json',
        xhrFields: { withCredentials: true  },
        /* 请求带上cookie */
        crossDomain: true, 
        // 跨域
        // contentType:'application/json',
        beforeSend : function(){ }, 
        // 发送请求前执行
        complete : function(jqxhr,status){ }, 
        // 请求完成完后(无论成功与否)执行
        cache : true , 
        // 后续可能会调用缓存;false则不缓存,只对post方法有效,默认为true
      })
      .then(function(backData,statusTxt,xhr){
        console.log('某接口',statusTxt,backData);
      }
      ,function(xhr,status,errorTrown){
        console.log("%cstatus:"+status+'某接口',"background:#afffee;");
      }) """
  '-jq-05-ajaxStart(cfoo);':
    'prefix': 'ajaxStart'
    'body': 'ajaxStart(function(){$1})'
  '-jq-05-ajaxStop(cfoo);':
    'prefix': 'ajaxStop'
    'body': 'ajaxStop(function(){$1})'
